<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>
  <body>
    <header><%- include('../partials/header'); %></header>

    <main class="w-screen">
      <div class="w-full">
        <div class="grid grid-cols-4 gap-5 ml-24 mr-24 mt-5 mb-24">
          <% categories.forEach(function(category) { %>
          <div class="mt-3">
            <div class="w-full aspect-square border">
              <img src="https://s.udemycdn.com/home/top-categories/lohp-category-design-2x-v2.jpg" />
            </div>
            <div class="font-semibold text-lg"><%= category.name%></div>
          </div>
          <% }) %>
        </div>
      </div>
    </main>

    <footer><%- include('../partials/footer'); %></footer>
  </body>
</html>
